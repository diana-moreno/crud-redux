{
  "name": "crudredux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "json-server": "^0.15.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "0.9.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "node-sass": "^4.13.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --runInBand --no-cache",
    "test-debug": "react-scripts inspect test --runInBand --no-cache",
    "sass": "sass template/index.sass template/index.css",
    "sass:watch": "sass --watch template/index.sass template/index.css",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
